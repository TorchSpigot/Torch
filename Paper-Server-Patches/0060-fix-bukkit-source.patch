-        }
-        if (depend != null) {
-            map.put("depend", depend);
-        }
-        if (softDepend != null) {
-            map.put("softdepend", softDepend);
-        }
-        if (website != null) {
-            map.put("website", website);
-        }
-        if (description != null) {
-            map.put("description", description);
-        }
-
-        if (authors.size() == 1) {
